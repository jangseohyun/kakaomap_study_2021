<script type="text/javascript" src="//dapi.kakao.com/v2/maps/sdk.js?appkey=a76949096713e82c75fa45bdb32ebb4c"></script>

<script type="text/javascript">

   function initialize()
   {
      container = document.getElementById("map");
      mapCenter = new kakao.maps.LatLng(37.556583, 126.919521);
      options =
      {
         center: mapCenter
         , level: 3
      }
      
      map = new kakao.maps.Map(container, options);
      
      // 마커 옵션 구성
      markerCenter = new kakao.maps.LatLng();
      
      // 마커 생성
      marker.setMap(map);
   }
   
</script>

</head>
<body onload="initialize()">

<div>
   <h1>Kakao Map 인포 윈도우 구성</h1>
   <hr>
</div>

<!-- 지도를 그릴 div 공간 확보(지정) -->
<div id="map" style="width: 100%; height: 350px;"></div>


